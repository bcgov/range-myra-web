{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "plugin:import/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "requireConfigFile": false
  },
  "env": {
		"browser": true,
		"node": true,
    "es6": true
	},
  "rules": {
    "react/prop-types": 0
  }
}
